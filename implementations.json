{
	"aioquic": {
		"image": "aiortc/aioquic-qns",
		"url": "https://github.com/aiortc/aioquic",
		"role": [
			"client",
			"server"
		],
		"client": {
			"type": "docker"
		}
	},
	"qns": {
		"image": "martenseemann/quic-network-simulator",
		"url": "https://github.com/marten-seemann/quic-network-simulator",
		"role": [
			"shaper"
		],
		"active": true,
		"scenarios": [
			"\"simple-p2p --delay=15ms --bandwidth=10Mbps --queue=25\""
		]
	},
	"chrome": {
		"url": "https://www.google.com/chrome/",
		"role": [
			"client"
		],
		"client": {
			"type": "application",
			"command": "google-chrome-unstable --origin-to-force-quic-on={request_url} --enable-experimental-web-platform-features --ignore-certificate-errors-spki-list={cert_fingerprint} https://{request_url}"
		}
	}
}