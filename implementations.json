{
	"aioquic": {
		"image": "aiortc/aioquic-qns",
		"url": "https://github.com/aiortc/aioquic",
		"role": [
			"server"
		],
		"client": {
			"type": "docker"
		}
	},
	"qns": {
		"image": "martenseemann/quic-network-simulator",
		"url": "https://github.com/marten-seemann/quic-network-simulator",
		"role": [
			"shaper"
		],
		"active": true,
		"scenarios": {
			"simple-p2p": {
				"arguments": "\"simple-p2p --delay=15ms --bandwidth=10Mbps --queue=25\"",
				"active": true
			}
		}
	},
	"chrome": {
		"url": "https://www.google.com/chrome/",
		"role": [
			"client"
		],
		"client": {
			"type": "application",
			"command": "google-chrome-unstable --origin-to-force-quic-on={origin} --enable-experimental-web-platform-features --auto-open-devtools-for-tabs --ignore-certificate-errors-spki-list={cert_fingerprint} {request_urls}"
		}
	},
	"firefox-nightly": {
		"url": "https://www.mozilla.org/en-US/firefox/",
		"role": [
			"client"
		],
		"client": {
			"type": "application",
			"command": "firefox-nightly {request_urls}"
		}
	},
	"chrome_unstable_94.0.4595.0": {
		"url": "https://www.google.com/chrome/",
		"role": [
			"client"
		],
		"client": {
			"type": "application",
			"command": "./applications/chrome-unstable-94.0.4595.0/chrome --origin-to-force-quic-on={origin} --enable-experimental-web-platform-features --auto-open-devtools-for-tabs --ignore-certificate-errors-spki-list={cert_fingerprint} {request_urls}",
			"setup": [
				{
					"sudo": true,
					"replace_tilde": true,
					"command": "rm -r ~/.cache/google-chrome-unstable/"
				},
				{
					"sudo": true,
					"replace_tilde": true,
					"command": "rm -r ~/.cache/google-chrome/"
				},
				{
					"sudo": false,
					"replace_tilde": true,
					"command": "python ./util/chrome-set-downloads-folder.py ~/.config/google-chrome/Default/Preferences \"{client_log_dir}\""
				},
				{
					"sudo": false,
					"replace_tilde": true,
					"command": "python ./util/chrome-set-downloads-folder.py ~/.config/google-chrome-unstable/Default/Preferences \"{client_log_dir}\""
				}
			]
		}
	},
